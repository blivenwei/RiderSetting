<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.pyc;*.pyo;*.rbc;*.yarb;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn;vssver.scc;vssver2.scc" />
    <extensionMap>
      <mapping ext="csx" type="C#" />
      <mapping ext="sh" type="PLAIN_TEXT" />
      <removed_mapping ext="txt" approved="true" type="PLAIN_TEXT" />
      <mapping pattern="*.lua.txt" type="lua" />
    </extensionMap>
  </component>
</application>